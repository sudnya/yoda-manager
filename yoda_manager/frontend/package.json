{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "^11.3.0",
    "@material-ui/core": "^4.12.3",
    "@mui/material": "^5.0.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "axios": "^0.22.0",
    "react": "^17.0.2",
    "react-data-table-component": "^7.3.1",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "react-toastify": "^8.0.3",
    "reactstrap": "^8.10.0",
    "styled-components": "^5.3.1",
    "web-vitals": "^1.1.2",
    "xlsx": "^0.17.2"
  },
  "scripts": {
    "start": "react-scripts start",
    "start:server-dev": "concurrently \"npm start\" \"cross-env FLASK_ENV=development FLASK_APP=../backend/app.py flask run\"",
    "start:server-prod": "rm -rf ../backend/templates && rm -rf ../backend/static && npm run build && cross-env FLASK_ENV=production FLASK_APP=../backend/app.py flask run",
    "build": "react-scripts build && mv build ../backend && mv ../backend/build/static ../backend && mv ../backend/build ../backend/templates",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "concurrently": "^6.2.2",
    "cross-env": "^7.0.3"
  }
}
